[Unit]
Description=Fan Safety Initialization (Max fans at boot)
DefaultDependencies=no
After=sysinit.target
Before=multi-user.target

[Service]
Type=oneshot
User=root
ExecStart=/bin/bash /usr/local/bin/fan-init.sh
RemainAfterExit=yes
StandardOutput=journal
StandardError=journal
SyslogIdentifier=fan-init

[Install]
WantedBy=multi-user.target
